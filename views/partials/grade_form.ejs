<form name="form2" id="form2" action="" method="POST">
    <div class="row g-3 form-top">
        <div class="col-sm-4">
            <input type="text" class="form-control" id="userName" name="userName" placeholder="Name *"
                aria-label="userName" required>
        </div>
        <div class="col-sm-4">
            <input type="text" class="form-control" placeholder="Roll Number *" id="rollNumber" name="rollNumber"
                aria-label="rollNumber" maxlength="8" required />
            <div class="row">
                <div class="col-sm-6">
                    <p style="color: red;" id="roll"></p>
                </div>
                <div class="col-sm-6">
                    <p style="color: red;" id="year"></p>
                </div>
            </div>
        </div>
        <div class="col-sm">
            <input type="text" class="form-control" id="phoneNumber" name="phoneNumber" placeholder="Mobile Number *"
                aria-label="phoneNumber" maxlength="10" required />
            <p style="color: red;" id="phone"></p>
        </div>
    </div>
    <div class="row g-3 form-top">
        <div class="col-sm-4">
            <select class="form-select" id="programme" name="programme" aria-label="Programme" required>
                <option value="" selected>Choose Programme *</option>
            </select>
        </div>
        <div class="col-sm-4">
            <select class="form-select" aria-label="discipline" name="discipline" id="discipline" required>
                <option value="" selected>Choose Discipline (Choose Programme First) *</option>
            </select>
        </div>
        <div class="col-sm-4">
            <select class="form-select" aria-label="Batch" name="batch" id="batch">
